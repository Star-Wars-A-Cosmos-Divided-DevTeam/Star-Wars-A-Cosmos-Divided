		&<hangar/hangar_modular/hangar_taxiway/Hangar_Bay_2x3.rules>/Part
		&<hangar/hangar_modular/hangar_taxiway/Hangar_Bay_3x5.rules>/Part
		&<hangar/hangar_modular/hangar_taxiway/Hangar_Bay_5x5.rules>/Part
		&<hangar/hangar_modular/hangar_taxiway/Hangar_Bay_5x10.rules>/Part
		&<hangar/hangar_modular/hangar_taxiway/Hangar_Bay_10x20.rules>/Part
		&<hangar/hangar_modular/hangar_taxiway/Hangar_Bay_20x40.rules>/Part
	]
	RenderLayers
	[
		// Floors.
		{
			Key = "SW_hangar_floors"// Ship bucket layer.
			Value // The actual material.
			{
				UniqueBucket = -1000
				RenderStage = Low
				AtlasTextureParams
				{
					MipLevels = 7 // Assumes 64x64 min part sprite size.
				}
				Material
				{
					Shader = "./Data/ships/common/parts.shader"
				}
				GhostMaterial
				{
					Shader = "./Data/ships/common/parts_ghost.shader"
				}
				NormalsMaterial
				{
					Shader = "./Data/ships/common/no_normals.shader"
				}
			}
		}
		// Roofs.
		{
			Key = "SW_hangar_roofs"// Ship bucket layer.
			Value // The actual material.
			{
				UniqueBucket = 1250
				RenderStage = Highest
				IsRoof = true
				HasNormals = true
				AtlasTextureParams
				{
					MipLevels = 7 // Assumes 64x64 min part sprite size.
				}
				Material
				{
					Shader = "./Data/ships/common/roof_colored_lit.shader"
				}
				DiffuseMaterial
				{
					Shader = "./Data/ships/common/roof_colored.shader"
				}
				NormalsMaterial
				{
					Shader = "./Data/ships/common/normals.shader"
				}
			}
		}
	]
}