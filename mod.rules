		CreateIfNotExisting = true
		ToAdd = "SW.structure1x3_wedge_R"
	}
	// Adding A STRUCTURE TRIANGLE TO THE VANILLA ARMOR TRIANGLE WHEN DESTROYED 
	{ 	//38
		Action = Overrides
		OverrideIn = "<ships/terran/armor_tri/armor_tri.rules>/Part"
