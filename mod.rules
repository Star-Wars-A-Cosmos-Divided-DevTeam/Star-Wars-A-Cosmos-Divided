		ManyToAdd
		[
			{
				Category = "limit_1" //Globally per Ship 
				Max = 1
			}
			{
				Category = "limit_1Cockpits" 
				Max = 1
			}
			{
				Category = "XWingThruster" 
				Max = 1
			}
			{
				Category = "limit_2" //Globally per Ship
				Max = 2
			}
			{
				Category = "XWingThrusterSmall" 
				Max = 2
			}
			{
				Category = "limit_2Cockpits"
				Max = 2
			}
			{
				Category = "limit_4" //Globally per Ship
				Max = 4
			}

			{
